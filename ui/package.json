{
  "name": "angular2-spring-boot",
  "version": "0.0.1",
  "scripts": {
    "sleep": "sleep 3",
    "clean": "rimraf -rf ./dist ../src/main/resources/static",
    "precompile": "npm run clean",
    "compile": "webpack --config=./config/webpack.config.babel.js",
    "ghpages": "npm run compile -- --env ghpages",

    "json-server": "babel-node ./node_modules/.bin/json-server --watch -r ./config/json-server/routes.json -c ./config/json-server/config.json ./config/json-server/testdata.js",

    "serve": "npm-run-all build start-server",
    "build": "npm run compile -- --env production",
    "start-server": "browser-sync start -c ./config/bs-config.js",
    "server": "npm run start-server -- --no-notify --no-open --no-online --no-inject-changes --no-reload-on-restart",

    "open": "open-url localhost:8000",
    "open-url": "npm-run-all sleep open",
    "start": "npm-run-all -p webpack-dev-server open-url",
    "webpack": "npm run compile -- --env development",
    "webpack-dev-server": "webpack-dev-server -w -d --config=./config/webpack.config.babel.js --env development",

    "stop": "kill -9 $(ps waux | grep browser-sync | grep -v grep | awk '{print $2}') | true",

    "lint": "tslint --ignoreBoundClassMethods --noUnusedParameters --noUnusedLocals -t verbose -c ./config/tslint.json 'src/**/*.ts'",

    "pretest": "npm run webpack",
    "test": "concurrently 'npm run protractor' 'npm run server' --kill-others --success first -p '[{name}]' -n 'E2E,HTTP' -c 'green,cyan' # colors: black, red, green, yellow, blue, magenta, cyan, white, gray",
    "protractor": "protractor ./config/protractor.js",
    "preprotractor": "npm-run-all sleep webdriver-manager-update",
    "webdriver-manager-update": "webdriver-manager update",

    "up": "npm-run-all docker-compose:up api-server-up",
    "down": "true | echo &(npm-run-all api-server:stop docker-compose:down)",

    "docker-compose:up": "docker-compose up -d",
    "docker-compose:down": "docker-compose down",

    "api-server": "bash ../gradlew -b ../build.gradle ",
    "api-server:status": "npm run api-server -- --status",
    "api-server:up": "npm run api-server -- build bootRun",
    "api-server-up": "true | (concurrently -p '[{name}]' -n 'API' -c 'bgGreen.bold' 'npm run api-server:up' --kill-others --success first &)",
    "api-server:stop": "npm run api-server -- --stop",
    "api-server:rebuild": "npm run api-server -- clean build -x test"
  },
  "dependencies": {
    "@angular/common": "2.4.8",
    "@angular/core": "2.4.8",
    "@angular/forms": "2.4.8",
    "@angular/http": "2.4.8",
    "@angular/platform-browser-dynamic": "2.4.8",
    "@angular/router": "3.4.8",
    "@types/core-js": "0.9.35",
    "bootstrap": "3.3.7",
    "es6-promise": "4.0.5",
    "es6-shim": "0.35.3",
    "ie-shim": "0.1.0",
    "jquery": "3.1.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.2.0",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@angular/compiler": "2.4.8",
    "@angular/compiler-cli": "2.4.8",
    "@angular/platform-browser": "2.4.8",
    "@angular/platform-server": "2.4.8",
    "@angular/upgrade": "2.4.8",
    "@ngtools/webpack": "1.2.11",
    "@types/chai": "3.4.35",
    "@types/jasmine": "2.5.43",
    "@types/node": "7.0.5",
    "@types/selenium-webdriver": "2.53.40",
    "@types/source-map": "0.5.0",
    "@types/uglify-js": "2.6.28",
    "@types/webpack": "2.2.7",
    "angular-cli": "1.0.0-beta.28.3",
    "angular2-router-loader": "0.3.5",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "6.7.6",
    "awesome-typescript-loader": "3.0.8",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",

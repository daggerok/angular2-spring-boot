{
  "name": "angular2-spring-boot-security",
  "version": "0.0.1",
  "scripts": {
    "off.json-server": "babel-node ./node_modules/.bin/json-server --watch -r ./config/json-server/routes.json -c ./config/json-server/config.json ./config/json-server/testdata.js",
    "off.json-server.start-server-and-open-prod-url": "npm-run-all -p open-prod-url json-server",
    "off.json-server.start": "npm-run-all -p webpack-dev-server json-server open-dev-url",
    "off.open-prod-url": "npm-run-all sleep open-prod",
    "off.start-server-and-open-prod-url": "npm-run-all -p open-prod-url",
    "off.open-prod": "open-url localhost:8080",

    "sleep": "sleep 3",
    "clean": "rimraf -rf ./dist",
    "prebuild": "npm run clean",
    "prestart": "npm i",
    "predev": "npm i",

    "open-dev": "open-url localhost:8000",
    "open-dev-url": "npm-run-all sleep open-dev",

    "serve": "npm-run-all build start-server",
    "start-server": "browser-sync start -c ./config/bs-config.js",

    "lint": "tslint 'src/**/*.ts' -c ./config/tslint.json --noUnusedParameters --noUnusedLocals",

    "dev": "npm-run-all -p watch start-server",
    "watch": "npm run webpack -- -w",
    "start.ui": "npm-run-all -p webpack-dev-server open-dev-url",
    "start": "npm-run-all -p webpack-dev-server",

    "compile": "webpack --config=./config/webpack.config.babel.js -c --process --bail",
    "build": "NODE_ENV=production npm run compile",
    "webpack": "NODE_ENV=development npm run compile",
    "webpack-dev-server": "NODE_ENV=development webpack-dev-server -w -d --config=./config/webpack.config.babel.js --content-base=./src",

    "test": "npm run e2e",
    "e2e": "npm-run-all -p prepare-e2e wait-and-run-protractor",
    "pree2e": "webdriver-manager update",
    "prepare-e2e": "concurrently -p '[{name}]' -n 'HTTP' -c 'bgYellow.bold' 'npm run webpack-dev-server' --kill-others --success first",
    "wait-and-run-protractor": "concurrently -p '[{name}]' -n 'PROTRACTOR' -c 'bgGreen.bold' 'npm-run-all wait protractor' --kill-others --success first",
    "postwait-and-run-protractor": "npm run stop",
    "wait": "sleep 25",
    "protractor": "protractor ./config/protractor.js",
    "stop": "kill -9 $(ps waux | grep webpack-dev-server | grep -v color | awk '{print $2}') | true",

    "up": "npm-run-all docker-compose:up api-server-up",
    "down": "true | echo &(npm-run-all api-server:stop docker-compose:down)",

    "docker-compose:up": "docker-compose up -d",
    "docker-compose:down": "docker-compose down",

    "api-server": "bash ../gradlew -b ../build.gradle ",
    "api-server:status": "npm run api-server -- --status",
    "api-server:up": "npm run api-server -- build bootRun",
    "api-server-up": "true | (concurrently -p '[{name}]' -n 'API' -c 'bgGreen.bold' 'npm run api-server:up' --kill-others --success first &)",
    "api-server:stop": "npm run api-server -- --stop",
    "api-server:rebuild": "npm run api-server -- clean build -x test"
  },
